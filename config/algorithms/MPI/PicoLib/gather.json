{
  "swing_over": {
    "desc": "Libswing external swing algorithm. It uses a binomial tree pattern of communication choosing its peer utilizing `swing` peer selection method `pi`. It works only if `comm` is a power of 2 and root is 0.",
    "version": "1.0.0",
    "constraints": [
      {
        "key": "count",
        "conditions": [
          {
            "operator": ">=",
            "value": "comm_sz"
          }
        ]
      },
      {
        "key": "comm_sz",
        "conditions": [
          {
            "operator": "is_power_of_two",
            "value": true
          }
        ]
      },
      {
        "key": "root",
        "conditions": [
          {
            "operator": "==",
            "value": 0
          }
        ]
      }
    ],
    "selection": "pico",
    "tags": [
      "swing",
      "external"
    ]
  }
}
